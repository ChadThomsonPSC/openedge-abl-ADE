<html>
<head>
<title>Progress Dynamics</title>
<HTA:APPLICATION ID="oMyApp" 
    APPLICATIONNAME="Dynamics" 
    BORDER="dialog"
    CAPTION="yes"
    ICON="../img/icfdev.ico"
    SHOWINTASKBAR="yes"
    SINGLEINSTANCE="yes"
    SYSMENU="yes"
    WINDOWSTATE="normal">
<link rel="stylesheet" type="text/css" href="../dhtml/rymain.css">
<script language="javascript">
  // Uncomment the following line when *not using* extension mapping
  // window.dyn="/scripts/cgiip.exe/WService=wsdynamics1/dhtml"; 
  // Uncomment the following line when *using* extension mapping
  window.dyn="";

  // Extract the menu from the querystring (if there is one)
  var cQryStr = window.location.search.substr(1);
  var aQryStr = cQryStr.split('&');
  var cMenu = '';
  for (var i=0;i<aQryStr.length;i++){
    aQryStr[i] = aQryStr[i].split('=');
    if (aQryStr[i][0].toUpperCase() == 'ICFOBJ')
      cMenu = aQryStr[i][1];
  }
  // If not specified, show Progress Dynamics Administration menu
  if (cMenu=='')
    cMenu='afallmencw';

function run(){
  window.mainmenu=window.menubar;  // Menu entries from MenuController (ie, File etc -> menubar)
  window.mainband=window.menubar;  // Menu entries from MenuController (ie Programs  -> tabmenu)
  window.progmenu=window.menubar;  // Local menubar entries from programs 
  window.progband=window.menubar;  // Main toolbar
  apph.action('prg.'+cMenu);
}
function beforeLogin(){
}
function afterLogin(){
}
function home(){
  apph.action('prg.'+cMenu);
}

</script>
</head>
<body height="52" onLoad="run()"> 
<div id="menu" target="toolbar" page="0" 
  level="|2|2|2"
  actions="|app.home|main.info.toggle|app.debug.toggle"
  labels="|<img & src='../img/home.gif' title='Return to default window'>|<img & src='../img/statusu.gif' title='Toggle Status Window'>|<img & src='../img/debugu.gif' title='Toggle Debug Help'>"
</div>
<div id="menu" target="progmenu" page="0" 
  level="|1|2|2|2"
  actions="|dev|app.home|main.info.toggle|app.debug.toggle"
  labels="|Dev|<img & src='../img/home.gif' title='Return to default window'>Home|<img & src='../img/statusu.gif' title='Toggle Status Window'>Status|<img & src='../img/debugu.gif' title='Toggle Debug Help'>Debug"
</div>
<div id="menu" target="mainmenu" page="0" 
  level="|1|2|2|2"
  actions="|dev|app.home|main.info.toggle|app.debug.toggle"
  labels="|Dev|<img & src='../img/home.gif' title='Return to default window'>Home|<img & src='../img/statusu.gif' title='Toggle Status Window'>Status|<img & src='../img/debugu.gif' title='Toggle Debug Help'>Debug"
</div>
<table cellspacing="0" cellpadding="0" width="100%" height="100%">
  <tr>
    <td class="toolbar" border="1">
      <div id="menubar"></div>
    </td>
  </tr>
  <tr>
    <td class="toolbar" border="1">
      <div id="toolbar" page="0"></div>
    </td>
  </tr>
  <tr height="100%" valign="top">
    <td colspan="2">
      <div id="app"></div>
    </td>
  </tr>  
  <tr id="inforow" height="100" style="display:none;"> 
    <td>
      <table cellspacing="0" cellpadding="0" width="100%">
        <tr>
          <td>
            <div id="info" block="inforow"></div>
          </td>
        </tr>  
      </table>
    </td>
  </tr>  
</table>
</body>
</html>
